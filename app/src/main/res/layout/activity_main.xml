                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="로그인"
                    android:layout_centerVertical="true"
                    android:layout_alignLeft="@+id/searchLayout"
                    android:layout_alignStart="@+id/searchLayout"
                    android:layout_marginLeft="213dp"
                    android:layout_marginStart="213dp"
                    android:visibility="visible"/>

                <ImageView
                    android:id="@+id/userPhoto"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/icon_user_person"
                    android:visibility="gone"
                    android:layout_alignTop="@+id/searchLayout"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="10dp"
                    android:layout_marginEnd="10dp" />

                <Button
                    android:id="@+id/mNotyCount"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
